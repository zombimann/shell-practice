#include <stdio.h>
#include "main.h"

/**
 * main - a simple shell that runs commands with their full path without
 * and commandline argument
 *
 * Return: always zero
 */

int main(int argc, char *argc[])
{
	ssize_t getlineretval;
	char **lineptr;
	size_t *n;
	FILE *stream;

	while(1)
	{
		printf("Zombi$ ");
		lineptr = NULL;
		*stream = stdin;
		*n = MAXLINE;
		getlineretval = getline(*);
	}
	free(lineptr);
	return (0);
}
